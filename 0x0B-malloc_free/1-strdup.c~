#include "main.h"
#include <stdlib.h>
/**
*_stdrup - function that returns a pointer to a newly allocated space
*@str: pointer ro char
*
*Return: NULL if str = NULL
*
*/
char *_strdup(char *str)
{
int x;
char *ptr;
if (str == NULL)
return (NULL);
ptr = (char *)malloc(sizeof(char));
for (x = 0; x < strlen(ptr + 1); x++)
str[x] = ptr[x];

if (ptr == NULL)
return (NULL);
else
return (str);

free(ptr);
return (0);
}
